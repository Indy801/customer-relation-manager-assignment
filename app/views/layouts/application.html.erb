<!DOCTYPE html>
<html>
  <head>
    <title>CustomerRelationManager</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="links">
      <ul>
        <li><%= link_to 'Home', root_path %></li>
        <li><%= link_to 'Alphabetized', alphabetized_customers_path %></li>
        <li><%= link_to 'Missing Email', missing_email_customers_path %></li>
      </ul>
    </div>
    <%= yield %>
  </body>
</html>
